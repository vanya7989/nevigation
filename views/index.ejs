function getCoordinates(event) {
  var box = document.getElementById('container');
  var boxRect = box.getBoundingClientRect();
  var offsetX2 = event.clientX - boxRect.left;
  var offsetY2 = event.clientY - boxRect.top;
  var k = 176064.5619057447;
  var xk = offsetX2 / k;
  var yk = offsetY2 / k;
  var x = xk + 81.5194368;
  var y = yk + 16.5637309;
