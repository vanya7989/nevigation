  // Create a new div element (box) at the clicked location
  var newBox = document.createElement("div");
  newBox.className = "smallBox";
  newBox.id = "box_" + counter;
  counter++;

  // Position the new box at the click coordinates
  newBox.style.left = offsetX1 + "px";
  newBox.style.top = offsetY1 + "px";

  // Remove previously marked location if any
  if (lastMarkedLocation) {
      mainBox.removeChild(lastMarkedLocation);
  }

  // Append the new box inside the main box
  mainBox.appendChild(newBox);

  // Update the last marked location
  lastMarkedLocation = newBox;
}

